package main

func foo()(out []i32) {
	out = []i32{1, 2, 3}
}

type barStruct struct {
	index i32
}

func bar()(out barStruct) {
	out.index = 4
}

func main ()() {
	var ff []i32 = foo()
	test(ff[2], 3, "")
	test(foo()[2], 3, "")

	var bb barStruct = bar()
	test(bb.index, 4, "")
	test(bar().index, 4, "")
}
