package main

type Point struct {
	name str
	isTrue bool
	x i32
	y i32
}

func main () {
	byt1 := 44B
	byt2 := 47B
	test(byt1, 44B, "")
	test(byt2, 47B, "")

	bool1 := true
	bool2 := false
	test(bool1, true, "")
	test(bool2, false, "")

	num1 := 5
	num2 := 10
	test(num1, 5, "")
	test(num2, 10, "")

	num3 := 33L
	num4 := 55L
	test(num3, 33L, "")
	test(num4, 55L, "")

	dec1 := 5.0
	dec2 := 10.0
	test(dec1, 5.0, "")
	test(dec2, 10.0, "")

	dec3 := 33.0D
	dec4 := 55.0D
	test(dec3, 33.0D, "")
	test(dec4, 55.0D, "")

	p1 := Point{
		x: 10,
		y: 11,
		isTrue: true,
		name: "Pointy"}
	p2 := Point{
		x: 20,
		y: 21,
		isTrue: false,
		name: "Punto"}

	test(p1.name, "Pointy", "")
	test(p1.x, 10, "")
	test(p1.y, 11, "")
	test(p1.isTrue, true, "")

	test(p2.name, "Punto", "")
	test(p2.x, 20, "")
	test(p2.y, 21, "")
	test(p2.isTrue, false, "")
}
